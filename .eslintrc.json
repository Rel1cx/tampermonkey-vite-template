{
    "extends": [
        "with-tsconfig"
    ],
    "root": true,
    "env": {
        "node": false,
        "browser": true,
        "es2021": true
    },
    "rules": {
        "etc/prefer-less-than": "off"
    },
    "ignorePatterns": [
        "node_modules/",
        "dist/"
    ],
    "overrides": [
        {
            "files": [
                "vite.config.ts"
            ],
            "parserOptions": {
                "project": "tsconfig.node.json"
            }
        }
    ]
}